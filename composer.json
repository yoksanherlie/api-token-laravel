{
    "name": "silverbullet/api-token-laravel",
    "description": "Token-based API service authentication & authorization for Laravel",
    "keywords": ["api", "api token", "authentication", "service", "authorization", "laravel"],
    "homepage": "https://github.com/yoksanherlie/api-token-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Yoksan Herlie",
            "email": "yoksanh@gmail.com"
        }
    ],
    "require": {
        "php" : "^7.2",
        "illuminate/support": "^5.2|^6|^7|^8",
        "illuminate/http": "^5.2|^6|^7|^8"
    },
    "require-dev": {
        "illuminate/console": "^5.2|^6|^7|^8",
        "illuminate/database": "^5.2|^6|^7|^8"
    },
    "autoload": {
        "psr-4": {
            "Silverbullet\\ApiTokenLaravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Silverbullet\\ApiTokenLaravel\\Providers\\ApiTokenServiceProvider"
            ]
        }
    }
}
